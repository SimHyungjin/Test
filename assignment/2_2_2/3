#include <iostream>
using namespace std;

class Point {
	double x;
	double y;
public:
	double getX()
	{
		return x;
	}
	double getY()
	{
		return y;
	}
	Point() {}
	Point(int x, int y) :x(x), y(y) {}
	~Point() {};
	void setX(int x)
	{
		this->x = x;
	}
	void setY(int y)
	{
		this->y = y;
	}
	void printPoint()
	{
		cout << "point x = " << x << endl;
		cout << "point y = " << y << endl;
	}

};

int main(int argc, char const* argv[])
{
	Point A(5, 6);
	A.printPoint();

	return 0;
}
